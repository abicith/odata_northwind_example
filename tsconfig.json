{
  "compilerOptions": {
    "noImplicitAny": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "target": "es6",
    "baseUrl": "./",
    "paths": {
      "@servers/*": [
        "servers/northwind-service/src/*"
      ]
    },
    "allowUnreachableCode": false
  }

}
